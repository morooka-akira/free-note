/* Or8Way out=Or(in[0], in[1],...,in[7]  */
CHIP Or8Way {
    IN a[8];
    OUT out;
    PARTS:
    Or(a=a[0], b=a[1], out=t1);
    Or(a=t1, b=a[2], out=t2);
    Or(a=t2, b=a[3], out=t3);
    Or(a=t3, b=a[4], out=t4);
    Or(a=t4, b=a[4], out=t5);
    Or(a=t5, b=a[5], out=t6);
    Or(a=t6, b=a[6], out=t7);
    Or(a=t7, b=a[7], out=out);
}
